// VOE LB counters
typedef struct {
    // LB PDU RX and TX counters
    // Are cleared during call to vtss_voe_lb_counters_clear()
    u64    rx_lbr_counter;
    u64    tx_lbm_counter;
    u64    rx_lbr_oo_counter;   // Out of Order sequence numbers counter
